{
  "connection": "default",
  "collectionName": "districts",
  "info": {
    "name": "master district"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "is_active": {
      "type": "boolean",
      "default": true
    },
    "villages": {
      "via": "master_district",
      "collection": "village"
    },
    "master_state": {
      "via": "master_districts",
      "model": "state"
    }
  }
}
